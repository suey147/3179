{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Though Vega-Lite supports only one scale per axes, one can create a parallel coordinate plot by folding variables, using `joinaggregate` to normalize their values and using ticks and rules to manually create axes.",
    "data": {
        "url": "https://raw.githubusercontent.com/suey147/3179/HomeworkWeek10/data/AI_index_db.csv"
    },
    "width": 600,
    "height": 300,
    "transform": [
      {"fold": ["Infrastructure", "Operating Environment", "Research", "Development", "Government Strategy"]},
      {
        "joinaggregate": [
          {"op": "min", "field": "value", "as": "min"},
          {"op": "max", "field": "value", "as": "max"}
        ],
        "groupby": ["Country"]
      },
      {
        "calculate": "(datum.min + datum.max) / 2",
        "as": "mid"
      }
    ],
    "layer": [{
      "mark": {"type": "rule", "color": "#ccc"},
      "encoding": {
        "detail": {"aggregate": "count"},
        "x": {"field": "key"}
      }
    }, {
      "mark": "line",
      "encoding": {
        "color": {"type": "nominal", "field": "Country"},
        "opacity": {"value": 0.3},
        "x": {"type": "nominal", "field": "key"},
        "y": {"type": "quantitative", "field": "value", "axis": null},
        "tooltip": [{
          "type": "nominal",
          "field": "Country"
        },
        {
          "type": "quantitative",
          "field": "value",
          "title": "score"
        }
        ]
      }
    }, {
      "encoding": {
        "x": {"type": "nominal", "field": "key"},
        "y": {"value": 0}
      },
      "layer": [{
        "mark": {"type": "text", "style": "label"},
        "encoding": {
          "text": {"aggregate": "max", "field": "max"}
        }
      }, {
        "mark": {"type": "tick", "style": "tick", "size": 8, "color": "#ccc"}
      }]
    }, {
      "encoding": {
        "x": {"type": "nominal", "field": "key"},
        "y": {"value": 150}
      },
      "layer": [{
        "mark": {"type": "text", "style": "label"},
        "encoding": {
          "text": {"aggregate": "min", "field": "mid"}
        }
      }, {
        "mark": {"type": "tick", "style": "tick", "size": 8, "color": "#ccc"}
      }]
    }, {
      "encoding": {
        "x": {"type": "nominal", "field": "key"},
        "y": {"value": 300}
      },
      "layer": [{
        "mark": {"type": "text", "style": "label"},
        "encoding": {
          "text": {"aggregate": "min", "field": "min"}
        }
      }, {
        "mark": {"type": "tick", "style": "tick", "size": 8, "color": "#ccc"}
      }]
    }],
    "config": {
      "axisX": {"domain": false, "labelAngle": 0, "tickColor": "#ccc", "title": null},
      "view": {"stroke": null},
      "style": {
        "label": {"baseline": "middle", "align": "right", "dx": -5},
        "tick": {"orient": "horizontal"}
      }
    }
  }